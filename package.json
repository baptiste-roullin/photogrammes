{
  "name": "tout-ce-qui-bouge",
  "version": "0.2.0",
  "engines": {
    "node": "^20.11.1"
  },
  "scripts": {
    "dev:eleventy": "eleventy --incremental --serve",
    "debug:eleventy": "DEBUG=* eleventy",
    "dev:postcss": "postcss src/assets/css/main.css -o dist/assets/css/main.css --watch",
    "dev": "npm-run-all  --parallel dev:* --print-label",
    "build:postcss": "cross-env NODE_ENV=production postcss --verbose src/assets/css/main.css -o dist/assets/css/main.css",
    "build:eleventy": "cross-env NODE_ENV=production eleventy  --incremental --quiet",
    "build": "run-s  build:* --print-label"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0-alpha.5",
    "@11ty/eleventy-img": "^4.0.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.2.2",
    "postcss-hash": "^2.0.0",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "postcss-nested": "^5.0.3",
    "tailwindcss": "^2.2.7"
  },
  "dependencies": {
    "alpinejs": "^3.12.0",
    "photoswipe": "*"
  },
  "_moduleAliases": {
    "@src": "./src",
    "@root": "./"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead"
      }
    ]
  ]
}